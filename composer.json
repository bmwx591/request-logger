{
    "name": "bmwx591/request-logger",
    "description": "Library for logging application requests",
    "keywords": ["psr7", "logger", "request"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bmwx591\\RequestLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bmwx591\\RequestLogger\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Aslanov",
            "email": "bumerx591@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4||^8.0",
        "psr/log": "^1.1",
        "psr/http-message": "^2.0"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
